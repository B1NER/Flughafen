<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>

<Pane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="1024.0"
      prefWidth="1280.0" stylesheets="@CSSFiles/BuchenStyle.css" xmlns="http://javafx.com/javafx/8"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.ZuBearbeitendenNutzerFindenConroller">
    <children>
        <StackPane prefHeight="1024.0" prefWidth="1280.0">
            <children>
                <ImageView fitHeight="1024.0" fitWidth="1280.0" pickOnBounds="true" preserveRatio="true" scaleY="1.44">
                    <image>
                        <Image url="@Grafiken/Background.jpg"/>
                    </image>
                </ImageView>
                <VBox prefHeight="1024.0" prefWidth="1280.0" spacing="10.0">
                    <children>
                        <Label fx:id="NuterbearbeitenText" alignment="CENTER" prefHeight="193.0" prefWidth="1278.0"
                               text="Nutzer bearbeiten" textFill="#fffefe">
                            <font>
                                <Font size="46.0"/>
                            </font>
                        </Label>
                        <Label fx:id="SuchenText" alignment="TOP_LEFT" prefHeight="47.0" prefWidth="132.0"
                               text="Suchen:" textFill="#fffefe">
                            <font>
                                <Font size="32.0"/>
                            </font>
                            <VBox.margin>
                                <Insets left="70.0"/>
                            </VBox.margin>
                        </Label>
                        <HBox alignment="CENTER" fillHeight="false" prefHeight="0.0" prefWidth="1278.0" spacing="10.0">
                            <children>
                                <Label fx:id="VornameText" alignment="TOP_CENTER" text="Vorname" textFill="#fffefe">
                                    <font>
                                        <Font size="17.0"/>
                                    </font>
                                    <HBox.margin>
                                        <Insets/>
                                    </HBox.margin>
                                </Label>
                                <TextField fx:id="VornameFeld" alignment="TOP_CENTER">
                                    <HBox.margin>
                                        <Insets/>
                                    </HBox.margin>
                                </TextField>
                                <Label fx:id="NachnameText" alignment="TOP_CENTER" text="Nachname" textFill="#fffefe">
                                    <font>
                                        <Font size="17.0"/>
                                    </font>
                                    <HBox.margin>
                                        <Insets/>
                                    </HBox.margin>
                                </Label>
                                <TextField fx:id="NachnameFeld" alignment="TOP_CENTER">
                                    <HBox.margin>
                                        <Insets/>
                                    </HBox.margin>
                                </TextField>
                                <Button fx:id="SuchenButton" mnemonicParsing="false" onAction="#SuchenAction"
                                        prefHeight="50.0" prefWidth="200.0" text="Suchen"/>
                            </children>
                        </HBox>
                        <TableView prefHeight="723.0" prefWidth="1280.0">
                            <columnResizePolicy>
                                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                            </columnResizePolicy>
                            <columns>
                                <TableColumn fx:id="SpalteVorname" prefWidth="75.0" text="Vorname"/>
                                <TableColumn fx:id="SpalteNachname" prefWidth="142.0" text="Nachname"/>
                                <TableColumn fx:id="SpalteGeburtsdatum" prefWidth="128.0" text="Geburtsdatum"/>
                                <TableColumn fx:id="SpaltEmail" prefWidth="115.0" text="E-Mail"/>
                            </columns>
                            <VBox.margin>
                                <Insets top="20.0"/>
                            </VBox.margin>
                        </TableView>
                        <HBox alignment="CENTER" prefHeight="243.0" prefWidth="1280.0" spacing="20.0">
                            <children>
                                <Button fx:id="LoschenButton" alignment="CENTER" contentDisplay="CENTER"
                                        mnemonicParsing="false" onAction="#LoschenAction" prefWidth="200.0"
                                        text="Löschen" textOverrun="CENTER_ELLIPSIS">
                                    <HBox.margin>
                                        <Insets/>
                                    </HBox.margin>
                                </Button>
                                <Button fx:id="BearbeitenButton" mnemonicParsing="false" onAction="#BearbeitenAction"
                                        prefWidth="200.0" text="Bearbeiten">
                                    <HBox.margin>
                                        <Insets/>
                                    </HBox.margin>
                                </Button>
                                <Button fx:id="ZuruckButton" mnemonicParsing="false" onAction="#ZuruckAction"
                                        prefWidth="200.0" text="Zurück">
                                    <HBox.margin>
                                        <Insets/>
                                    </HBox.margin>
                                </Button>
                            </children>
                        </HBox>
                    </children>
                </VBox>
            </children>
        </StackPane>
    </children>
</Pane>
