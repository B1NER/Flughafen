<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<StackPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
           minWidth="-Infinity"
           prefHeight="1024.0" prefWidth="1280.0" stylesheets="@../View/CSSFiles/Style.css"
           xmlns="http://javafx.com/javafx/8"  fx:controller="Controller.ZuBearbeitendenNutzerFindenConroller">
    <children>
        <ImageView fitHeight="1024.0" fitWidth="1280.0" pickOnBounds="true" preserveRatio="true" scaleY="1.44">
            <image>
                <Image url="@../View/Grafiken/Background.jpg"/>
            </image>
        </ImageView>
        <VBox prefHeight="1024.0" prefWidth="1280.0" spacing="10.0">
            <children>
                <Label fx:id="NuterbearbeitenText" alignment="CENTER" prefHeight="150.0" prefWidth="1278.0"
                       text="Nutzer finden" textFill="#fffefe">
                    <font>
                        <Font size="46.0"/>
                    </font>
                </Label>
                <HBox alignment="CENTER" fillHeight="false" layoutY="20.0" prefHeight="50.0" prefWidth="1278.0"
                      spacing="10.0">
                    <children>
                        <Label fx:id="VornameText" alignment="TOP_CENTER" prefHeight="50.0" text="Vorname:"
                               textFill="#fffefe">
                            <font>
                                <Font size="36.0"/>
                            </font>
                            <HBox.margin>
                                <Insets/>
                            </HBox.margin>
                        </Label>
                        <TextField fx:id="VornameFeld" alignment="CENTER" prefHeight="50.0">
                            <HBox.margin>
                                <Insets/>
                            </HBox.margin>
                            <font>
                                <Font size="25.0"/>
                            </font>
                        </TextField>
                        <Label fx:id="NachnameText" alignment="TOP_CENTER" prefHeight="50.0" text="Nachname:"
                               textFill="#fffefe">
                            <font>
                                <Font size="36.0"/>
                            </font>
                            <HBox.margin>
                                <Insets/>
                            </HBox.margin>
                        </Label>
                        <TextField fx:id="NachnameFeld" alignment="CENTER" prefHeight="50.0">
                            <HBox.margin>
                                <Insets/>
                            </HBox.margin>
                            <font>
                                <Font size="25.0"/>
                            </font>
                        </TextField>
                        <Button fx:id="SuchenButton" mnemonicParsing="false" onAction="#SuchenAction"
                                prefHeight="50.0" prefWidth="200.0" text="Suchen">
                            <font>
                                <Font size="25.0"/>
                            </font>
                        </Button>
                    </children>
                </HBox>
                <TableView fx:id="tabelle" prefHeight="631.0" prefWidth="1280.0">
                    <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                    </columnResizePolicy>
                    <columns>
                        <TableColumn fx:id="SpalteVorname" prefWidth="75.0" text="Vorname"/>
                        <TableColumn fx:id="SpalteNachname" prefWidth="327.0" text="Nachname"/>
                        <TableColumn fx:id="SpalteGeburtsdatum" prefWidth="312.0" text="Geburtsdatum"/>
                        <TableColumn fx:id="SpaltEmail" prefWidth="115.0" text="E-Mail"/>
                        <TableColumn fx:id="spalteTyp" prefWidth="75.0" text="Typ"/>
                    </columns>
                    <VBox.margin>
                        <Insets top="20.0"/>
                    </VBox.margin>
                </TableView>
                <HBox alignment="TOP_CENTER" prefHeight="110.0" prefWidth="1280.0" spacing="20.0">
                    <children>
                        <Button fx:id="LoschenButton" alignment="CENTER" contentDisplay="CENTER"
                                mnemonicParsing="false" onAction="#LoschenAction" prefHeight="65.0"
                                prefWidth="200.0" text="Löschen" textOverrun="CENTER_ELLIPSIS">
                            <HBox.margin>
                                <Insets top="10.0"/>
                            </HBox.margin>
                            <font>
                                <Font size="20.0"/>
                            </font>
                        </Button>
                        <Button fx:id="BearbeitenButton" mnemonicParsing="false" onAction="#BearbeitenAction"
                                prefHeight="65.0" prefWidth="200.0" text="Bearbeiten">
                            <HBox.margin>
                                <Insets top="10.0"/>
                            </HBox.margin>
                            <font>
                                <Font size="20.0"/>
                            </font>
                        </Button>
                        <Button fx:id="ZuruckButton" mnemonicParsing="false" onAction="#ZuruckAction"
                                prefHeight="65.0" prefWidth="200.0" text="Zurück">
                            <HBox.margin>
                                <Insets top="10.0"/>
                            </HBox.margin>
                            <font>
                                <Font size="20.0"/>
                            </font>
                        </Button>
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
</StackPane>
