<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<StackPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
           minWidth="-Infinity" prefHeight="1024.0" prefWidth="1280.0" stylesheets="@../View/CSSFiles/Style.css"
           xmlns="http://javafx.com/javafx/8" fx:controller="Controller.AdminStartseiteController">
    <children>
        <ImageView fitHeight="1024.0" fitWidth="1280.0" pickOnBounds="true" preserveRatio="true" scaleY="1.44">
            <image>
                <Image url="@../View/Grafiken/Background.jpg"/>
            </image>
        </ImageView>
        <VBox fx:id="AdministartorText" alignment="CENTER" prefHeight="1024.0" prefWidth="1280.0" spacing="20.0">
            <children>
                <HBox prefHeight="130.0" prefWidth="1280.0" spacing="330.0">
                    <children>
                        <Label alignment="CENTER" text="Administrator" textFill="#f8f5f5">
                            <font>
                                <Font size="61.0"/>
                            </font>
                            <padding>
                                <Insets left="450.0"/>
                            </padding>
                        </Label>
                    </children>
                </HBox>
                <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Label fx:id="FlugListeText" alignment="CENTER" text="Flug Liste" textAlignment="CENTER"
                               textFill="#f8f5f5">
                            <font>
                                <Font size="36.0"/>
                            </font>
                        </Label>
                    </children>
                </HBox>
                <TableView fx:id="tabelle" prefHeight="650.0" prefWidth="1280.0">
                    <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                    </columnResizePolicy>
                    <columns>
                        <TableColumn fx:id="SpalteID" prefWidth="75.0" text="Flug ID"/>
                        <TableColumn fx:id="SpalteStartort" prefWidth="163.0" text="Startort"/>
                        <TableColumn fx:id="SpalteZielort" prefWidth="75.0" text="Zielort"/>
                        <TableColumn fx:id="SpalteStartzeit" prefWidth="75.0" text="Startzeit"/>
                        <TableColumn fx:id="SpalteAnkunftszeit" prefWidth="75.0" text="Ankunftszeit"/>
                        <TableColumn fx:id="SpalteFluggesellschaft" prefWidth="75.0" text="Fluggesellschaft"/>
                        <TableColumn fx:id="SpalteGepaeckskapazitaet" prefWidth="75.0" text="Gepaeckskapazität"/>
                        <TableColumn fx:id="SpalteSitzplaetze" prefWidth="75.0" text="Sitzplätze"/>
                        <TableColumn fx:id="SpaltePreis" prefWidth="75.0" text="Preis (pro Person)"/>
                        <TableColumn fx:id="SpalteStatus" prefWidth="75.0" text="Status"/>
                    </columns>
                    <padding>
                        <Insets left="5.0" right="5.0"/>
                    </padding>
                </TableView>
                <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
                    <children>
                        <Button fx:id="AngestelltehinzufugenButton" mnemonicParsing="false"
                                onAction="#NutzerHinzufugenAction" prefHeight="65.0" prefWidth="230.0"
                                text="Nutzer erstellen">
                            <font>
                                <Font size="20.0"/>
                            </font>
                        </Button>
                        <Button fx:id="BenutzerBearbeitenButton" mnemonicParsing="false"
                                onAction="#BenutzerBearbeitenAction" prefHeight="65.0" prefWidth="230.0"
                                text="Nutzer bearbeiten">
                            <font>
                                <Font size="20.0"/>
                            </font>
                        </Button>
                        <Button fx:id="BuchungBearbeitenButton" alignment="CENTER" mnemonicParsing="false"
                                onAction="#BuchungBearbeitenAction" prefHeight="65.0" prefWidth="230.0"
                                text="Buchungen bearbeiten">
                            <font>
                                <Font size="20.0"/>
                            </font>
                        </Button>
                        <Button fx:id="AbmeldenButton" alignment="CENTER" mnemonicParsing="false"
                                onAction="#AbmeldenAction" prefHeight="65.0" prefWidth="230.0" text="Abmelden">
                            <font>
                                <Font size="20.0"/>
                            </font>
                        </Button>
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
</StackPane>
