<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<StackPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
           minWidth="-Infinity"
           prefHeight="1024.0" prefWidth="1280.0" stylesheets="@../View/CSSFiles/Style.css"
           xmlns="http://javafx.com/javafx/8" fx:controller="Controller.ProfilBearbeitenController">
    <children>
        <ImageView fitHeight="1024.0" fitWidth="1280.0" pickOnBounds="true" preserveRatio="true" scaleY="1.44">
            <image>
                <Image url="@../View/Grafiken/Background.jpg"/>
            </image>
        </ImageView>
        <VBox alignment="CENTER" prefHeight="1023.0" prefWidth="1278.0">
            <children>
                <Label fx:id="PersonbearbeitenText" text="Profil bearbeiten" textFill="#fcf9f9">
                    <font>
                        <Font size="68.0"/>
                    </font>
                </Label>
                <GridPane alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                          minWidth="-Infinity" prefHeight="600.0" prefWidth="800.0">
                    <children>
                        <Label fx:id="VornameText" text="Vorname" textAlignment="CENTER" textFill="#fff9f9"
                               GridPane.columnIndex="1">
                            <font>
                                <Font size="30.0"/>
                            </font>
                        </Label>
                        <Label fx:id="GeburtsdatumText" text="Geburtsdatum" textAlignment="CENTER"
                               textFill="#fff9f9" GridPane.columnIndex="1" GridPane.rowIndex="2">
                            <font>
                                <Font size="30.0"/>
                            </font>
                        </Label>
                        <Label fx:id="NachnameText" text="Nachname" textAlignment="CENTER" textFill="#fff9f9"
                               GridPane.columnIndex="1" GridPane.rowIndex="1">
                            <font>
                                <Font size="30.0"/>
                            </font>
                        </Label>
                        <Label fx:id="PasswordText" text="Passwort" textFill="#fff9f9" GridPane.columnIndex="1"
                               GridPane.rowIndex="3">
                            <font>
                                <Font size="30.0"/>
                            </font>
                        </Label>
                        <Label fx:id="Password2Text" text="Passwort bestätigen" textFill="#fff9f9"
                               GridPane.columnIndex="1" GridPane.rowIndex="4">
                            <font>
                                <Font size="30.0"/>
                            </font>
                        </Label>
                        <TextField fx:id="VornameFeld" prefHeight="65.0" prefWidth="380.0"
                                   GridPane.columnIndex="2" GridPane.columnSpan="2"/>
                        <TextField fx:id="NachnameFeld" prefHeight="65.0" prefWidth="380.0"
                                   GridPane.columnIndex="2" GridPane.columnSpan="2" GridPane.rowIndex="1"/>
                        <DatePicker fx:id="GeburtsdatumFeld" prefHeight="65.0" prefWidth="380.0"
                                    GridPane.columnIndex="2" GridPane.columnSpan="2" GridPane.rowIndex="2"/>
                        <PasswordField fx:id="Password" prefHeight="65.0" prefWidth="380.0"
                                       GridPane.columnIndex="2" GridPane.columnSpan="2" GridPane.rowIndex="3"/>
                        <PasswordField fx:id="Password2Feld" prefHeight="65.0" prefWidth="380.0"
                                       GridPane.columnIndex="2" GridPane.columnSpan="2" GridPane.rowIndex="4"/>
                    </children>
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="144.0" minWidth="3.0" prefWidth="14.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="321.0" minWidth="10.0"
                                           prefWidth="321.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="223.0" minWidth="0.0" prefWidth="0.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="333.8011169433594" minWidth="10.0"
                                           prefWidth="266.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="223.0" minWidth="6.50555419921875"
                                           prefWidth="73.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    </rowConstraints>
                </GridPane>
                <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="20.0">
                    <children>
                        <Button fx:id="AbbrechenButton" mnemonicParsing="false" onAction="#AbbrechenAction"
                                prefHeight="65.0" prefWidth="200.0" text="Abbrechen">
                            <font>
                                <Font size="20.0"/>
                            </font>
                        </Button>
                        <Button fx:id="BestatigenButton" mnemonicParsing="false" onAction="#BestatigenAction"
                                prefHeight="65.0" prefWidth="200.0" text="Bestätigen">
                            <font>
                                <Font size="20.0"/>
                            </font>
                        </Button>
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
</StackPane>

